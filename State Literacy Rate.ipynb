    "file ='D:/OneDrive/Documents/Studies/Programming/Python/data/StateLevelLiteracyRate.json'\n",
    "state, years=[], []\n",
    "\n",
    "with open (file) as StateDataFile:\n",
    "    data = json.load(StateDataFile)\n",
    "    \n",
    "    for line in data['fields']:\n",
    "        years.append(line['label'])\n",
    "        \n",
    "    for line in data['data']:\n",
    "        state.append(line[0])\n",
    "        \n",
    "years.remove('State/ UTs')\n",
    "\n",
    "df = pd.DataFrame.from_dict(data['data'], orient='columns')\n",
    "df.set_index(0, inplace=True)\n",
    "df.columns=years\n",
    "\n",
    "df = df.replace('NA', np.NaN)\n",
    "df = df.astype(float)\n",
    "\n",
    "df = df.fillna(method='bfill', axis='columns')\n",
    "df"
   ]
