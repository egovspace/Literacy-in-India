    "    title = \"Statewise Literacy Rate\",\n",
    "    yaxis = dict(\n",
    "     title = '% Literacy'\n",
    "    ),\n",
    "    xaxis = dict(\n",
    "     title = 'Years'\n",
    "    )\n",
    ")\n",
    "\n",
    "def literacyPlot(state):\n",
    "    stat = []\n",
    "    for st in state:\n",
    "        trace = go.Scatter( \n",
    "            x = years, \n",
    "            y = df.loc[st], \n",
    "            mode = 'lines', \n",
    "            name = st\n",
    "        )\n",
    "        \n",
    "        stat.append(trace)\n",
    "    \n",
    "    fig = go.Figure(data = stat, layout = layout)\n",
    "    py.offline.iplot (fig)\n",
